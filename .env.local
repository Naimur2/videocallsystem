# Local Development Environment
# Copy this to .env for local development

# Database
DATABASE_URL=postgresql://mediasoup:password@postgres:5432/mediasoup
REDIS_URL=redis://redis:6379

# Security (change these!)
JWT_SECRET=your-local-jwt-secret-32-characters-long
SESSION_SECRET=your-local-session-secret-32-characters

# TURN Server (use public STUN for local dev)
TURN_USERNAME=
TURN_CREDENTIAL=
STUN_SERVER=stun:stun.l.google.com:19302

# MediaSoup
MEDIASOUP_ANNOUNCED_IP=127.0.0.1
RTC_MIN_PORT=40000
RTC_MAX_PORT=49999

# App Config
NODE_ENV=development
DOMAIN=localhost
FRONTEND_PORT=3000
BACKEND_PORT=3001